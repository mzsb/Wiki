<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Black">#000000</Color>
    <Color x:Key="White">#FFFFFF</Color>
    <Color x:Key="Gray">#938F8F</Color>
    <Color x:Key="LightBlue">#F3F5F7</Color>
    <Color x:Key="LightGray">#dadfe2</Color>
    <Color x:Key="LinkBlue">#479dc8</Color>

    <Color x:Key="ButtonLightBlue">#e6ecef</Color>
    <Color x:Key="ButtonLightGray">#88989b</Color>

    <Color x:Key="ListViewItemBlue">#f7f9fa</Color>

    <SolidColorBrush  x:Key="LightGrayBrush" Color="{StaticResource LightGray}"/>

</ResourceDictionary>
